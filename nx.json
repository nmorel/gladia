{
  "defaultBase": "main",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "tscheck", "build"]
      }
    }
  },
  "targetDefaults": {
    "tscheck": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["prepare:dev", "^prepare:dev", "^build"]
    },
    "serve": {
      "dependsOn": ["prepare:dev", "^prepare:dev", "build", "^build"]
    },
    "test:e2e": {
      "dependsOn": ["prepare:test", "^prepare:test", "^build"]
    },
    "test:e2e:watch": {
      "dependsOn": ["prepare:test", "^prepare:test", "^build"]
    }
  }
}
