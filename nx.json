{
  "defaultBase": "main",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "tscheck", "build"]
      }
    }
  },
  "targetDefaults": {
    "tscheck": {
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["prepare:dev", "^build"]
    },
    "serve": {
      "dependsOn": ["prepare:dev", "build", "^build"]
    }
  }
}
